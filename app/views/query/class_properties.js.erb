$("#qb_properties_properties_loading").hide("fast");
var list_group = $("#property_main_properties_group");
<%@properties.each do |sc|%>
	var html = "<a href='javascript:void(0);' class='property-subclass-individual list-group-item' uri='<%=sc[:uri]%>' display-name='<%=sc[:name]%>' clicked='false' ><%=sc[:name]%></a>";
	$(list_group).append(html);
<%end%>